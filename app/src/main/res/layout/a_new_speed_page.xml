<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cf.g2utools.cheezling.activity.MainPages.NewSpeed">

    <cf.g2utools.cheezling.custom_view.BottomMenu
        android:id="@+id/bot"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent">

    </cf.g2utools.cheezling.custom_view.BottomMenu>

    <cf.g2utools.cheezling.custom_view.SmallTalkView
        android:id="@+id/smallTalkView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/bot"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </cf.g2utools.cheezling.custom_view.SmallTalkView>
</android.support.constraint.ConstraintLayout>
